package firstParcialExercises.Parcial1;

/**
 *
 * @author Alejandro
 */
public class NewMain {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        MyRectangle test = new MyRectangle(4,1,1,4);//creas puntos de rectangulo
        System.out.println(test.getl1());//prueba el rectangulo del Constructor con los parametros ints
        System.out.println(test.getl2());
        System.out.println(test);
        System.out.println(test.getArea());
        System.out.println(test.getPerimeter());
        MyPoint l1 = new MyPoint(3,0);//creas puntos de rectangulo
        MyPoint l2 = new MyPoint(0,3);
        MyRectangle test2 = new MyRectangle(l1,l2);//prueba el constructor con parametros MyPoint
        System.out.println(test2.getl1());
        System.out.println(test2.getl2());
        System.out.println(test2);
        System.out.println(test2.getArea());
        System.out.println(test2.getPerimeter());// TODO code application logic here
    }
    
}
